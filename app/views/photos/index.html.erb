<h1>All Photos</h1>
<div><a href="/photos/new">Add a new photo</a></div>

<% @all_photos.each do |photo| %>
<img src="<%= photo.source %>">
<p>
  <%= photo.caption %>
</p>
<div>
  <a href="/photos/<%= photo.id %>">Show details</a>
  <a href="/photos/<%= photo.id %>/edit">Edit</a>
  <a href="/delete_photo/<%= photo.id %>">Delete</a>
</div>
<% end %>
